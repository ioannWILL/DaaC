@startuml Test Use Case Diagram

scale 2
left to right direction
skinparam packageStyle rect
actor customer
actor clerk
actor anotherCustomer
anotherCustomer --|> customer
rectangle Checkout {
    customer -- checkout
    (checkout) .> (payment): include
    (help) .> (checkout): extend
    (checkout) -- clerk
    (decline) -- clerk
    anotherCustomer -- (payment)
}
@endumlgit 